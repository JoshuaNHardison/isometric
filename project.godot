config_version=5

[application]

config/name="Isometric Game"
config/description="This demo shows a traditional isometric view with depth sorting.

A character can move around the level and will also slide around objects,
as well as be occluded when standing in front or behind them."
config/tags=PackedStringArray("2d", "demo", "official", "tilemap")
run/main_scene="res://levels/dungeon.tscn"
config/features=PackedStringArray("4.2")
config/icon="res://icon.webp"

[autoload]

CowManager="*res://scripts/cow_manager.gd"
CowboyManager="*res://scripts/cowboy_manager.gd"

[display]

window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[file_customization]

folder_colors={
"res://levels/": "green",
"res://scenes/": "purple",
"res://scripts/": "red"
}

